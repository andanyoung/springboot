<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="andanyoung.springboot.shardingjdbc.mapper.TConfigMapper">

    <resultMap id="BaseResultMap" type="andanyoung.springboot.shardingjdbc.domain.TConfig">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="remark" column="remark" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="lastModifyTime" column="last_modify_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,remark,create_time,
        last_modify_time
    </sql>
</mapper>
